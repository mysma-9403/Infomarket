<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="AppBundle\Entity\MenuMenuEntryAssignment" 
        table="menu_menu_entry_assignments"
        repository-class="AppBundle\Repository\Admin\Assignments\MenuMenuEntryAssignmentRepository">
        
        <many-to-one field="menu" target-entity="Menu" inversed-by="menuMenuEntryAssignments" />
        
        <many-to-one field="menuEntry" target-entity="MenuEntry" inversed-by="menuMenuEntryAssignments" />
        
        <field name="orderNumber" type="integer" default="1" />
        
    </entity>
    
</doctrine-mapping>