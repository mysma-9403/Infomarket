{% if app.user %}
	<p>
		{{ 'label.welcome'|trans }} {{ app.user.username }},<br> 
		<a rel="nofollow" href="{{ path('fos_user_security_logout') }}">
			{{ 'action.logout'|trans }}
		</a>
	</p>
{% else %}
	<p>
		<a rel="nofollow" href="{{ path('fos_user_security_login') }}">{{ 'action.login'|trans }}</a>
	</p>
	<p>
		<a rel="nofollow" href="{{ path('fos_user_registration_register') }}">{{ 'action.register'|trans }}</a>
	</p>
{% endif %}