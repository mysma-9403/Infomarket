{% extends 'infomarket/base/index.html.twig' %}

{% set title = 'label.categories' %}
{% set type = 'label.categories' %}
{% set mainPath = 'category' %}

{% block mainContent %}
	<div class="row">
		<div class="col-xs-12">
			{% for category in entries %}
				<div class="col-sm-3">
					{% include 'common/tiles/bottom_rect_tile.twig' with { 
						entry: category, extPath: '_show' } %}
				</div>
			{% endfor %}
		</div>
	</div>
{% endblock %}
