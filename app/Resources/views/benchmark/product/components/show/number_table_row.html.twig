<tr>
	<th class="text-right">{{ field.fieldName }}</th>
	
	<td class="text-center">
		{% include 'benchmark/common/value/product_value.twig' with { value: field.value, field: field } %}
	</td>
	<td class="text-center text-success">
		{{ field.betterThan ? field.betterThan|number_format(0, '.', ' ') ~ '%' : '-' }}
	</td>
	<td class="text-center">
		{{ field.note ? field.note|number_format(1, '.', ' ') : '-' }}
	</td>
	
	<td></td>
</tr>