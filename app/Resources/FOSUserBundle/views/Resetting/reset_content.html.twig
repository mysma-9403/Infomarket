{% trans_default_domain 'FOSUserBundle' %} {% form_theme form
'form/fields.html.twig' %} {% set title = 'label.resetting' %}

<div class="form-register">
	{{ form_start(form, { 'action': path('fos_user_resetting_reset',
	{'token': token}), 'attr': { 'class': 'fos_user_resetting_reset' } })
	}} {{ form_widget(form) }}
	<div>
		<input class="btn btn-success btn-block" type="submit"
			value="{{ 'resetting.reset.submit'|trans }}" />
	</div>
	{{ form_end(form) }}
</div>